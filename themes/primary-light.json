  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9a9189",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#c7a511"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#c86565"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#c86565"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#c86565"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#3a3022"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#c7a511"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c7a511"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#66604f"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#66604f"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#c86565"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#3a3022"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#c7a511"
      }
    },
    {
      "name": "Module/Package",
      "scope": [
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ae3b3b"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#25779a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#c86565"
      }
    },
    {
      "name": "JSON - Key",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    },
    {
      "name": "CSS - Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#3a3022"
      }
    },
    {
      "name": "CSS - Class/ID",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#25779a"
      }
    }
  ]
